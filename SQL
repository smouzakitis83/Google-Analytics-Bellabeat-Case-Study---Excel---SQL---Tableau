-- Combined daily activity data from multiple months and casting the ID from integer to string for consistency and improved compatibility.
SELECT
  CAST(Id AS STRING) AS Id,
  *
EXCEPT(Id)
FROM
  `bellabeat-477019.Bellabeat_3_4.dailyActivity_merged_3_4`

UNION DISTINCT

SELECT
  CAST(Id AS STRING) AS Id,
  *
EXCEPT(Id)
FROM
  `bellabeat-477019.Bellabeat_4_5.dailyActivity_merged_4_5`;
